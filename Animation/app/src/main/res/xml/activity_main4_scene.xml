<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <ConstraintSet android:id="@+id/start">
        <Constraint
            android:layout_height="100dp"
            motion:layout_constraintEnd_toStartOf="@+id/imageView2"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintBottom_toTopOf="@+id/imageView4"
            motion:layout_constraintHorizontal_bias="0.5"
            android:layout_width="100dp"
            motion:layout_constraintHorizontal_chainStyle="packed"
            android:id="@+id/imageView1"
            motion:layout_constraintVertical_chainStyle="packed" />
        <Constraint
            android:layout_height="100dp"
            motion:layout_constraintEnd_toStartOf="@+id/imageView5"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintBottom_toTopOf="@+id/imageView7"
            motion:layout_constraintHorizontal_bias="0.5"
            android:layout_width="100dp"
            motion:layout_constraintTop_toBottomOf="@+id/imageView1"
            motion:layout_constraintHorizontal_chainStyle="packed"
            android:id="@+id/imageView4" />
        <Constraint
            android:layout_height="100dp"
            motion:layout_constraintEnd_toStartOf="@+id/imageView8"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintHorizontal_bias="0.5"
            android:layout_width="100dp"
            motion:layout_constraintTop_toBottomOf="@+id/imageView4"
            motion:layout_constraintHorizontal_chainStyle="packed"
            android:id="@+id/imageView7" />
        <Constraint
            android:layout_height="100dp"
            motion:layout_constraintStart_toEndOf="@+id/imageView2"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintBottom_toTopOf="@+id/imageView6"
            motion:layout_constraintHorizontal_bias="0.5"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="100dp"
            motion:layout_constraintVertical_chainStyle="packed"
            android:id="@+id/imageView3" />
        <Constraint
            android:layout_height="100dp"
            motion:layout_constraintEnd_toStartOf="@+id/imageView3"
            motion:layout_constraintStart_toEndOf="@+id/imageView1"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintBottom_toTopOf="@+id/imageView5"
            motion:layout_constraintHorizontal_bias="0.5"
            android:layout_width="100dp"
            motion:layout_constraintVertical_chainStyle="packed"
            android:id="@+id/imageView2" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
        <Constraint
            android:id="@+id/imageView1"
            motion:layout_constraintEnd_toStartOf="@+id/imageView2"
            android:layout_width="@dimen/hundredTwenty"
            android:layout_height="@dimen/hundredTwenty"
            motion:layout_constraintBottom_toTopOf="@+id/imageView4"
            motion:layout_constraintHorizontal_bias="0.5"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent" />
        <Constraint
            android:id="@+id/imageView2"
            motion:layout_constraintEnd_toStartOf="@+id/imageView3"
            motion:layout_constraintStart_toEndOf="@+id/imageView1"
            android:layout_width="@dimen/hundredTwenty"
            android:layout_height="@dimen/hundredTwenty"
            motion:layout_constraintBottom_toTopOf="@+id/imageView5"
            motion:layout_constraintHorizontal_bias="0.5"
            motion:layout_constraintTop_toTopOf="parent" />
        <Constraint
            android:id="@+id/imageView3"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toEndOf="@+id/imageView2"
            android:layout_width="@dimen/hundredTwenty"
            android:layout_height="@dimen/hundredTwenty"
            motion:layout_constraintBottom_toTopOf="@+id/imageView6"
            motion:layout_constraintHorizontal_bias="0.5"
            motion:layout_constraintTop_toTopOf="parent" />
        <Constraint
            android:id="@+id/imageView4"
            motion:layout_constraintEnd_toStartOf="@+id/imageView5"
            android:layout_width="@dimen/hundredTwenty"
            android:layout_height="@dimen/hundredTwenty"
            motion:layout_constraintBottom_toTopOf="@+id/imageView7"
            motion:layout_constraintTop_toBottomOf="@+id/imageView1"
            motion:layout_constraintHorizontal_bias="0.5"
            motion:layout_constraintStart_toStartOf="parent" />
        <Constraint
            android:id="@+id/imageView5"
            motion:layout_constraintEnd_toStartOf="@+id/imageView6"
            motion:layout_constraintStart_toEndOf="@+id/imageView4"
            android:layout_width="@dimen/hundredTwenty"
            android:layout_height="@dimen/hundredTwenty"
            motion:layout_constraintBottom_toTopOf="@+id/imageView8"
            motion:layout_constraintTop_toBottomOf="@+id/imageView2"
            motion:layout_constraintHorizontal_bias="0.5" />
        <Constraint
            android:id="@+id/imageView6"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toEndOf="@+id/imageView5"
            android:layout_width="@dimen/hundredTwenty"
            android:layout_height="@dimen/hundredTwenty"
            motion:layout_constraintBottom_toTopOf="@+id/imageView9"
            motion:layout_constraintTop_toBottomOf="@+id/imageView3"
            motion:layout_constraintHorizontal_bias="0.5" />
        <Constraint
            android:id="@+id/imageView7"
            motion:layout_constraintEnd_toStartOf="@+id/imageView8"
            android:layout_width="@dimen/hundredTwenty"
            android:layout_height="@dimen/hundredTwenty"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintTop_toBottomOf="@+id/imageView4"
            motion:layout_constraintHorizontal_bias="0.5"
            motion:layout_constraintStart_toStartOf="parent" />
        <Constraint
            android:id="@+id/imageView8"
            motion:layout_constraintEnd_toStartOf="@+id/imageView9"
            motion:layout_constraintStart_toEndOf="@+id/imageView7"
            android:layout_width="@dimen/hundredTwenty"
            android:layout_height="@dimen/hundredTwenty"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintTop_toBottomOf="@+id/imageView5"
            motion:layout_constraintHorizontal_bias="0.5" />
        <Constraint
            android:id="@+id/imageView9"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toEndOf="@+id/imageView8"
            android:layout_width="@dimen/hundredTwenty"
            android:layout_height="@dimen/hundredTwenty"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintTop_toBottomOf="@+id/imageView6"
            motion:layout_constraintHorizontal_bias="0.5" />
    </ConstraintSet>
    <Transition
        motion:constraintSetStart="@+id/start"
        motion:constraintSetEnd="@+id/end" >
        <OnClick motion:targetId="@+id/floatingActionButton2" />
    </Transition>
    <Transition
        motion:constraintSetStart="@+id/end"
        motion:constraintSetEnd="@+id/start" >
        <OnClick motion:targetId="@+id/imageView5" />
    </Transition>
</MotionScene>